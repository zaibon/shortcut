package components

type ButtonProp struct {
	Type string
}

templ Button(prop ButtonProp) {
	<button type={ prop.Type } class="text-white bg-cyan-600 hover:bg-cyan-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2  focus:outline-none">
		{ children... }
	</button>
}
